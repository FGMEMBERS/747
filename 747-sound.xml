    <x>-30.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <property>/engines/engine[0]/thrust_lb[0]</property>
    <type>log</type>
    <factor>0.015</factor>
    <offset>0.0</offset>
   </volume>
  </jetrumble>

