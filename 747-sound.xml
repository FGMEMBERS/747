<?xml version="1.0"?>
 
<PropertyList>

<fx>
<!-- Engine #1  -->
  <engine>
   <name>engine1</name>
   <path>Sounds/fan.wav</path>
   <mode>looped</mode>
   <property>/engines/engine[0]/running</property>
   <position>
    <x>-30.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <property>/engines/engine[0]/n1</property>
    <factor type="double">0.005</factor>
    <offset type="double">0.025</offset>
   </volume>
   <pitch>
    <property>/engines/engine[0]/n1</property>
    <type>log</type>
    <factor>0.5</factor>
    <offset>0.65</offset>
   </pitch>
  </engine>
  
  <engine>
   <name>whine1</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <property>/engines/engine[0]/running</property>
   <position>
    <x>-30.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <property>/engines/engine[0]/n1</property>
    <type>ln</type>
    <factor>0.04</factor>
    <offset>0.02</offset>
   </volume>
   <pitch>
    <property>/engines/engine[0]/n1</property>
    <factor>0.01</factor>
    <offset>0.15</offset>
   </pitch>
  </engine>

  <jetrumble>
   <name>jetrumble1</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <property>/engines/engine[0]/running</property>
